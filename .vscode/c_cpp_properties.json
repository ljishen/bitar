{
  "configurations": [
    {
      "name": "linux-clang-arm64",
      "cStandard": "c11",
      "compileCommands": "${workspaceFolder}/build-aarch64/compile_commands.json",
      "compilerPath": "/usr/bin/clang++",
      "configurationProvider": "ms-vscode.cmake-tools",
      "cppStandard": "c++17",
      "defines": [],
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src/include",
        "${workspaceFolder}/build-aarch64/vcpkg_installed/arm64-linux/include",
        "${workspaceFolder}/opt/aarch64/_deps/arrow-src/cpp/src",
        "${env:HOME}/opt/dpdk-v22.07-aarch64/include"
      ],
      "intelliSenseMode": "linux-clang-arm64"
    },
    {
      "name": "linux-clang-x64",
      "cStandard": "c11",
      "compileCommands": "${workspaceFolder}/build-x64/compile_commands.json",
      "compilerPath": "/usr/bin/clang++",
      "configurationProvider": "ms-vscode.cmake-tools",
      "cppStandard": "c++17",
      "defines": [],
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/src/include",
        "${workspaceFolder}/build-x64/vcpkg_installed/x64-linux/include",
        "${workspaceFolder}/opt/x64/_deps/arrow-src/cpp/src",
        "${env:HOME}/opt/dpdk-v22.07-x64/include"
      ],
      "intelliSenseMode": "linux-clang-x64"
    }
  ],
  "version": 4
}
