{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "bitar",
  "version": "0.0.1",
  "homepage": "https://github.com/ljishen/bitar",
  "description": "bitar is a library for accessing hardware compression/decompression accelerators",
  "builtin-baseline": "40511e2280b7f9b60830f0f5a1812eaebf53a668",
  "dependencies": [
    {
      "name": "cxxopts",
      "version>=": "3.0.0"
    },
    {
      "$explanation": ["Dependency for system-installed Apache Arrow library"],
      "name": "curl",
      "version>=": "7.83.1#2",
      "default-features": false
    },
    {
      "name": "fmt",
      "version>=": "8.1.1#1"
    },
    {
      "name": "magic-enum",
      "version>=": "0.8.0"
    },
    {
      "$explanation": ["Dependency for system-installed Apache Arrow library"],
      "name": "openssl"
    }
  ],
  "overrides": [
    {
      "name": "openssl",
      "version-string": "1.1.1n#1"
    }
  ],
  "features": {
    "dpdk": {
      "description": "Install Data Plane Development Kit (DPDK) with vcpkg",
      "dependencies": [
        {
          "name": "dpdk",
          "version>=": "22.03"
        }
      ]
    },
    "tests": {
      "description": "Build the tests",
      "dependencies": [
        {
          "name": "catch2",
          "version>=": "3.0.1#2"
        }
      ]
    }
  }
}
