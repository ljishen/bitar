{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "bitar",
  "version": "0.0.1",
  "homepage": "https://github.com/ljishen/bitar",
  "description": "bitar is a library for accessing hardware compression/decompression accelerators",
  "builtin-baseline": "c48c0c05bdaa7490585b10c83932b5a58722e463",
  "dependencies": [
    {
      "name": "magic-enum",
      "version>=": "0.8.0"
    },
    {
      "name": "fmt",
      "version>=": "8.1.1#1"
    },
    {
      "name": "cxxopts",
      "version>=": "3.0.0"
    }
  ],
  "overrides": [
    {
      "name": "re2",
      "version": "2020-10-01#1"
    }
  ],
  "features": {
    "vcpkg-arrow": {
      "description": "Install Apache Arrow from vcpkg",
      "dependencies": [
        {
          "name": "arrow",
          "version>=": "8.0.0",
          "default-features": false,
          "features": ["dataset", "parquet"]
        }
      ]
    },
    "system-arrow-deps": {
      "description": "Install dependencies for system-installed Apache Arrow",
      "dependencies": [
        {
          "name": "c-ares",
          "version>=": "1.18.1#1"
        },
        {
          "name": "curl",
          "version>=": "7.83.1#2",
          "default-features": false,
          "features": ["non-http"]
        },
        "re2"
      ]
    },
    "tests": {
      "description": "Build the tests",
      "dependencies": [
        {
          "name": "catch2",
          "version>=": "3.0.1#1"
        }
      ]
    }
  }
}
